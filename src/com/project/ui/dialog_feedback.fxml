<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane minHeight="250.0" minWidth="500.0" stylesheets="@../css/main.css" xmlns="http://javafx.com/javafx/8.0.112-ea" xmlns:fx="http://javafx.com/fxml/1">
    <StackPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
    <children>
      <BorderPane>
         <bottom>
              <HBox minWidth="263.0" spacing="10.0" BorderPane.alignment="CENTER">
                  <children>
                      <Button id="button" fx:id="btnSend" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" minWidth="90.0" text="Submit">
                     <HBox.margin>
                        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                     </HBox.margin>
                  </Button>
                      <Button id="button" fx:id="btnCancel" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" minWidth="90.0" text="Close">
                        <HBox.margin>
                           <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                        </HBox.margin></Button>
                  </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
              </HBox>
         </bottom>
         <center>
            <VBox BorderPane.alignment="CENTER">
               <children>
                    <HBox fx:id="hboxNewSession">
                        <children>
                            <Label alignment="CENTER_RIGHT" minWidth="80.0" text="Subject : ">
                              <HBox.margin>
                                 <Insets bottom="2.0" left="5.0" right="5.0" top="2.0" />
                              </HBox.margin></Label>
                            <TextField fx:id="textFieldSubject" minWidth="300.0">
                              <HBox.margin>
                                 <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                              </HBox.margin></TextField>
                        </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                    </HBox>
                  <HBox fx:id="hboxNewSession1">
                     <children>
                        <Label alignment="CENTER_RIGHT" minHeight="23.0" minWidth="80.0" text="Description : ">
                              <HBox.margin>
                                 <Insets bottom="2.0" left="5.0" right="5.0" top="2.0" />
                              </HBox.margin></Label>
                        <TextArea fx:id="textFieldDescription" minHeight="100.0" minWidth="300.0">
                              <HBox.margin>
                                 <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                              </HBox.margin></TextArea>
                     </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </HBox>
               </children>
            </VBox>
         </center>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </BorderPane>
    </children>
    </StackPane>
</AnchorPane>
